#include <dt-bindings/zmk/bt.h>

/ {
        keymap {
                compatible = "zmk,keymap";

                default_layer {
                        bindings = <
                        &studio_unlock &kp N1  &kp N2  &kp N3  &kp N4  &kp N5  &kp N6  &mo 1
                        &trans         &trans  &trans  &trans  &trans  &trans  &trans  &trans
